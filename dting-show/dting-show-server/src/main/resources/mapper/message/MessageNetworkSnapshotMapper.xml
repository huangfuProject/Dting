<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dting.show.server.mapper.MessageNetworkSnapshotMapper">

    <insert id="batchInsert">
        insert into
        message_network_snapshot(`instance_id`,`message_ip`,`collect_time`,`network_data_key`)
        values
        <foreach collection="messageNetworkSnapshotList" item="messageNetworkSnapshot" separator=",">
            (#{messageNetworkSnapshot.instanceId}, #{messageNetworkSnapshot.messageIp}, #{messageNetworkSnapshot.collectTime}, #{messageNetworkSnapshot.networkDataKey})
        </foreach>
    </insert>
</mapper>