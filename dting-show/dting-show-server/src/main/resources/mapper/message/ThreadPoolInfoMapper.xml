<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dting.show.server.mapper.ThreadPoolInfoMapper">
    <insert id="batchInsert">
        insert into thread_pool_info(`collect_key`,`thread_pool_name`,`active_count`,`core_count`,
        `max_count`,`queue_use_count`,`queue_total_size`, `busy_weight`)
        values
        <foreach collection="networkInfoList" item="networkInfo" separator=",">
            (#{networkDataKey}, #{networkName}, #{ipAddress}, #{subnetMask}, #{receivingDataByte},
            #{sendDataByte}, #{receivingDataPacketCount}, #{sendDataPacketCount}, #{receivingErrorDataPacketCount},
            #{sendErrorDataPacketCount}, #{receivingDiscardedDataPacketCount}, #{sendDiscardedDataPacketCount})
        </foreach>
    </insert>
</mapper>