<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dting.show.server.mapper.MessageThreadPoolSnapshotMapper">

    <insert id="batchInsert">
        insert into
        message_thread_pool_snapshot(`instance_id`,`message_ip`,`collect_time`,`collect_key`)
        values
        <foreach collection="messageThreadPoolSnapshotList" item="messageThreadPoolSnapshot" separator=",">
            (
                #{messageThreadPoolSnapshot.instanceId}, #{messageThreadPoolSnapshot.messageIp}, #{messageThreadPoolSnapshot.collectTime}, #{messageThreadPoolSnapshot.collectKey}
            )
        </foreach>
    </insert>
</mapper>