<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dting.show.server.mapper.ThreadPoolDetailedSnapshotMapper">
    <insert id="batchInsert">
        insert into thread_pool_detailed_snapshot(`collect_key`,`thread_pool_name`,`active_count`,`core_count`,
        `max_count`,`queue_use_count`,`queue_total_size`, `busy_weight`)
        values
        <foreach collection="threadPoolDetailedSnapshots" item="threadPoolDetailedSnapshot" separator=",">
            (
            #{threadPoolDetailedSnapshot.collectKey},
            #{threadPoolDetailedSnapshot.threadPoolName},
            #{threadPoolDetailedSnapshot.activeCount},
            #{threadPoolDetailedSnapshot.coreCount},
            #{threadPoolDetailedSnapshot.maxCount},
            #{threadPoolDetailedSnapshot.queueUseCount},
            #{threadPoolDetailedSnapshot.queueTotalSize},
            #{threadPoolDetailedSnapshot.busyWeight}
            )
        </foreach>
    </insert>
</mapper>